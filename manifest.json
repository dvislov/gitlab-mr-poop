{
  "name": "Gitlab MR ðŸ’©",
  "version": "0.1.0",
  "description": "Extension for leaving comments to bad code at gitlab merge requests",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://gitlab.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
